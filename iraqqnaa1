function fixI(C) {
        if (!1 != isIphone) {
                var S = $(C).position().top - (document.body.scrollHeight - window.innerHeight) - 10;
                S < document.body.scrollHeight + window.innerHeight,
                $(document.body).scrollTop(S)
        }
}
function blurI() {
        !1 == isIphone || $(document.body).scrollTop(0)
}
